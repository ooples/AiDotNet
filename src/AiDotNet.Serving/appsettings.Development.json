{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "AiDotNet.Serving": "Debug"
    }
  },
  "ServingOptions": {
    "Port": 5000,
    "BatchingWindowMs": 10,
    "MaxBatchSize": 100,
    "StartupModels": []
  },
  "TierEnforcementOptions": {
    "Enabled": true,
    "TierHeaderName": "X-AiDotNet-Tier",
    "DefaultTier": "Free"
  },
  "AttestationOptions": {
    "AllowUnverifiedAttestationInDevelopment": true
  }
}
